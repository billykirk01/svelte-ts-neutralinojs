{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/public/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 800,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/public/icon.png"
    }
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/neutralino.js",
    "binaryVersion": "4.4.0",
    "clientVersion": "3.3.0",
    "frontendLibrary": {
        "patchFile": "/index.html",
        "devUrl": "http://localhost:3000"
                            
    } 
  }
}
